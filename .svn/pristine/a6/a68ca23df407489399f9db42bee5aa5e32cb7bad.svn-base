


/** 配色 **/
.list-group-item {
  border: 1px solid #d9d9d9
}

.top-nav-bar {
  background-color: rgb(61, 167, 66);
}

.form-control {
  border: 1px solid #d9d9d9;
}

.user-profile-panel {
  background-color: #bcddc5;
}

.panel-default > .panel-heading {
  background-image: linear-gradient(to bottom, #fff 0px, #fff 100%);
}

.panel-default {
  border-color: #d9d9d9;
}

.btn-primary {
  background-image: linear-gradient(to bottom, #bcddc5 0px, #66cc66 100%);
  border-color: #d9d9d9;
}

.btn-primary.active, .btn-primary:active {
  background-color: #66cc66;
  border-color: #d9d9d9;
}

.btn-primary:focus, .btn-primary:hover {
  background-color: #66cc66;
}

.modal-footer {
  border-top: 1px solid #d9d9d9;
}

.text-message-textarea {
  border: 1px solid #d9d9d9;
}

.report-panel {
  background-image: linear-gradient(to bottom, #bcddc5 0px, #66cc66 100%);
}

.audio-message {
  border: 1px solid #d9d9d9;
}

#calendar .cal-month-box, #calendar .cal-year-box, .cal-week-box {
  border-color: #d9d9d9;
}

#calendar .cal-year-box [class*="span"], #calendar .cal-month-box [class*="cal-cell"] {
  border-right: 1px solid #d9d9d9;
}

#calendar .cal-year-box .row-fluid, #calendar .cal-month-box .cal-row-fluid {
  border-bottom: 1px solid #d9d9d9;
}

.table-striped > tbody > tr:nth-of-type(2n+1) {
  background-color: #d9d9d9;
}

.btn-primary.active.focus, .btn-primary.active:focus, .btn-primary.active:hover, .btn-primary.focus:active, .btn-primary:active:focus, .btn-primary:active:hover, .open > .dropdown-toggle.btn-primary.focus, .open > .dropdown-toggle.btn-primary:focus, .open > .dropdown-toggle.btn-primary:hover {
  background-color: #66cc66;
  border-color: #d9d9d9;
}

a.list-group-item:focus, a.list-group-item:hover, button.list-group-item:focus, button.list-group-item:hover {
  background-color: #ffffff;
}
