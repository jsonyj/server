    <script>

        
        $(document).ready(function(){
            var item = "";
            var flag = 0;
            $.ajax({
                type: "POST",
                url:"http://video.didano.com/video/client/post/channel_get_by_student/"+<{$studentId}>,
                dataType: 'JSON',
                success: function(rs){
                    if(!rs.success){
                        item = '<div class="no-video col-xs-12">网络异常，暂时无法播放</div>';
                    }else{
                        
                        $.each(rs.data, function(index, obj) {
                            if(obj.length==0){
                                item = '<div class="no-video col-xs-12">网络异常，暂时无法播放</div>';
                            }else{

                                for(var i = 0; i < obj.length; i++){ 
                                    if(obj[i].on){
                                        var clickUrl = "'"+"?c=parent&a=videoDetails&studentId="+<{$studentId}>+"&channelId="+obj[i].channelId+"'";
                                        item += '<div class="col-xs-6 padding-5-0-10-10" onclick="window.location.href='+clickUrl+'"><img class="width-100" src="./img/u8.jpg"><p class="padding-top-5">'+obj[i].name+'</p></div>';
                                    }else{
                                        item += '<div class="col-xs-6 padding-5-0-10-10" onclick="timeTips(this)" id="'+obj[i].start_ends+'"><div><img class="width-100" src="./img/u8.jpg"><div class="coverDiv">未到开放时间</div></div><p class="padding-top-5">'+obj[i].name+'</p></div>';
                                        flag = 1;
                                        
                                    }
                                }
                            }
                        });
                    } 
                    $('.margin-top-15').append(item);//内容添加到页面上。
                     if(flag == 1){
                        var imgWidth = $('img').width();
                        var imgHeight = imgWidth/700 * 466;
                        console.log(imgWidth);
                        console.log(imgHeight);
                        $('.coverDiv').width(imgWidth);
                        $('.coverDiv').height(imgHeight);
                        $('.coverDiv').css('line-height',imgHeight+'px'); 
                     }  
                    

                },
                error: function(){
                    console.log("错误");
                    item = '<div class="no-video col-xs-12">网络异常，暂时无法播放</div>';
                    $(".margin-top-15").append(item);//内容添加到页面上。
                }
            });

        })


        function timeTips(obj){
            // var timeStr = $(obj).attr('id');
            var timeArr = $(obj).attr('id').split(',');
            var time;
            var timeStr = "";
            for (time in timeArr)
            {
                timeStr += '<p  class="margin-b-0" style="text-indent: 6em;">'+timeArr[time]+'</p>';
            }
            var timeTipsStr = "温馨提示：亲，该视频通道仅在每天的："+timeStr+'<p class="margin-b-0" style="text-indent: 6em;">时间段内可以观看！</p>';
            $("#classmates-tips").modal('show');
                $('#classmates-tips').find('.modal-dialog').css({  
                'margin-top':'150px'});
                $('.modal-body').html(timeTipsStr);
        }

        function cancel(){
          $('#classmates-tips').modal('hide');
        }
  	</script>
			