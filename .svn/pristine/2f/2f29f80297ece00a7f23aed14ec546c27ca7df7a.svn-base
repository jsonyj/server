            <!--跳转到电话绑定-->
            <a href="?c=public&a=messageIndex&studentId=<{$studentId}>"><span class="xiaoxi-tishi hidden"></span><i class="iconfont iconfont-color-hei">&#xe614;</i><div>消息</div></a>
        </div>
