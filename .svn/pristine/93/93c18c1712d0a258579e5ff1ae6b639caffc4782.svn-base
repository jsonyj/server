.margin-bottom-5 {
  margin-bottom: 5px;
}

.no-skin #main-container .main-content .page-content .row form table tr td textarea.textareaheight{
    height: 480px;
}

.toolbar {
    margin-bottom: 5px;
}

.bootstrap-timepicker-widget.dropdown-menu.timepicker-orient-left.timepicker-orient-top.open {
  z-index: 9999;
}

.toolbar .btn-group > .btn.btn-sm > .caret {
  margin-top: 0px;
}

.statistic-info span {
  padding-right: 45px;
  padding-left: 5px;
}

.search-form > .table > tbody > tr > th {
  vertical-align: middle;
}
.main-container .main-content .main-content-inner .page-content .form-group fieldset.fieldset{
    border: 1px solid silver;margin: 0 2px;padding: 0.35em 0.625em 0.75em;
}
.main-container .main-content .main-content-inner .page-content .form-group fieldset.fieldset legend.legend{
    border:0px;margin-bottom: 0px;width: inherit;
}

legend label {
  padding: 5px 10px;
}

.form-control-select {
  width: 330px;
}

.modal-dialog {
  width: 800px;
}

#main-container .main-content .page-content table tr td table tr td,#main-container .main-content .page-content table tr td table tr th{
    padding: 5px;
}
.main-container .main-content .page-content .tab-content table tr td .edui-container .edui-editor-body div p img,.main-container .main-content .page-content fieldset table tr td div p img{
    width: inherit;
}
.product-list-title a:hover {
  text-decoration: none;
}
.edui-container, .edui-btn-toolbar, .edui-dialog-container {
  width:100%;
}
.glyphicon-danger {
  color: red;
}

.help-block {
 margin: 0px;
}

.table .table.table-striped.table-condensed {
  margin-bottom: 0px;
}
